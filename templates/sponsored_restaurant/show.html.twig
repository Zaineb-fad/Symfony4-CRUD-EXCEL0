{% extends 'base.html.twig' %}

{% block title %}SponsoredRestaurant{% endblock %}

{% block body %}
<div class="container">
<div class="row">
<div class="col-md-6">

    <h1>SponsoredRestaurant</h1>

    <table class="table table-condensed">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sponsored_restaurant.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ sponsored_restaurant.name }}</td>
            </tr>
            <tr>
                <th>StartDate</th>
                <td>{{ sponsored_restaurant.startDate ? sponsored_restaurant.startDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EndDate</th>
                <td>{{ sponsored_restaurant.endDate ? sponsored_restaurant.endDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sponsored_restaurant_index') }}">back to list</a>

    <a href="{{ path('sponsored_restaurant_edit', {'id': sponsored_restaurant.id}) }}">edit</a>

    {{ include('sponsored_restaurant/_delete_form.html.twig') }}
    </div></div></div>
{% endblock %}
